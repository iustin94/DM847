build_matrix <- function (data) {
matrixData <- NULL
matrixData <- cbind(data)
matrixData <- matrixData[,-c(5,6,7)]
matrixData <- cbind(matrixData, cluster=data.kmeans$cluster)
matrixData <- as.data.frame(matrixData)
header_names <- strsplit(as.character(unique(matrixData$measurement_name)), " ")
matrix <- matrix(0, nrow = nclust, ncol = length(header_names))
colnames(matrix) <- header_names
for(i in 1:nrow(matrixData)) {
row <- matrixData[i,]
index <- which(citrus_names == row$measurement_name)
old_row <- matrix[row$cluster,]
old_row[index] = 1
matrix[row$cluster,] = old_row
}
return(matrix)
}
